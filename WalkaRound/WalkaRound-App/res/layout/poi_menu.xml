<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/pullup_info_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/pois"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/pois"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/category_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="1"
            android:text="@string/bars_and_pubs"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="2"
            android:text="@string/cinema"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="3"
            android:text="@string/clubs_and_nightclubs"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="4"
            android:text="@string/fast_food"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="5"
            android:text="@string/food"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="6"
            android:text="@string/museum"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="7"
            android:text="@string/public_transportation"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="8"
            android:text="@string/shop"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="9"
            android:text="@string/sleeping_accomodation"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="10"
            android:text="@string/supermarket"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/category_11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:tag="11"
            android:text="@string/theatre"
            android:textAppearance="?android:attr/textAppearanceMedium" />

    </LinearLayout>

</RelativeLayout>
